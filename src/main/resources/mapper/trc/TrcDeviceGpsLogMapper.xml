<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.company.springboot.mapper.trc.TrcDeviceGpsLogMapper" >
  <resultMap id="BaseResultMap" type="com.company.springboot.entity.trc.TrcDeviceGpsLog" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="hr_id" property="hrId" jdbcType="INTEGER" />
    <result column="imei" property="imei" jdbcType="VARCHAR" />
    <result column="sn" property="sn" jdbcType="VARCHAR" />
    <result column="longitude" property="longitude" jdbcType="DECIMAL" />
    <result column="latitude" property="latitude" jdbcType="DECIMAL" />
    <result column="gather_time" property="gatherTime" jdbcType="TIMESTAMP" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />

  </resultMap>
  <sql id="Base_Column_List" >
    id, hr_id, imei, sn,longitude,latitude,gather_time,create_time
  </sql>

  <select id="selectByOrderId" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select
    gps.id,
    gps.hr_id hrId,
    gps.imei,
    gps.sn,
    gps.longitude,
    gps.latitude,
    gps.gather_time gatherTime,
    gps.create_time createTime
    from fdp_fault_dispatch_hr_relation hr
    left join trc_device_gps_log gps on hr.id=gps.hr_id
    where hr.flag_used=1 and hr.flag_deleted=0 and hr.flag_available=1
    and hr.dispatch_order_id = #{orderId,jdbcType=INTEGER}
    order by gps.gather_time
  </select>
</mapper>